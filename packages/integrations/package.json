{
  "name": "@ticketz/integrations",
  "version": "1.0.0",
  "description": "Integrações externas do sistema Ticketz-LeadEngine",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "scripts": {
    "typecheck": "tsc -p tsconfig.build.json --noEmit",
    "build": "tsup",
    "build:clean": "tsup --clean",
    "dev": "tsup --watch",
    "test": "vitest --passWithNoTests",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@hapi/boom": "^10.0.1",
    "@ticketz/core": "workspace:*",
    "@whiskeysockets/baileys": "^6.7.20",
    "qrcode": "^1.5.3",
    "qrcode-terminal": "^0.12.0"
  },
  "devDependencies": {
    "@types/qrcode": "^1.5.5",
    "tsup": "^8.5.0",
    "typescript": "^5.3.3",
    "vitest": "^1.1.0"
  },
  "typesVersions": {
    "*": {
      "*": [
        "dist/*",
        "src/*"
      ]
    }
  },
  "files": [
    "dist",
    "index.d.ts"
  ]
}
