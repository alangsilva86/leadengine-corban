{
  "name": "@ticketz/core",
  "version": "1.0.0",
  "description": "Dom√≠nio e casos de uso do sistema Ticketz-LeadEngine",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsup && rm -f tsconfig.build.tsbuildinfo && tsc --project tsconfig.build.json --emitDeclarationOnly",
    "build": "tsup && tsc --project tsconfig.build.json",
    "dev": "tsup --watch",
    "test": "vitest --passWithNoTests",
    "test:watch": "vitest --watch --passWithNoTests",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf dist tsconfig.build.tsbuildinfo"
  },
  "dependencies": {
    "date-fns": "^3.0.6",
    "lodash": "^4.17.21",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.202",
    "tsup": "^8.5.0",
    "typescript": "^5.3.3",
    "vitest": "^1.1.0"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./tickets": {
      "types": "./dist/tickets/index.d.ts",
      "import": "./dist/tickets/index.mjs",
      "require": "./dist/tickets/index.js"
    },
    "./leads": {
      "types": "./dist/leads/index.d.ts",
      "import": "./dist/leads/index.mjs",
      "require": "./dist/leads/index.js"
    }
  },
  "files": [
    "dist"
  ]
}
