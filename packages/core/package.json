{
  "name": "@ticketz/core",
  "version": "1.0.0",
  "description": "Dom√≠nio e casos de uso do sistema Ticketz-LeadEngine",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "index.d.ts",
  "exports": {
    ".": {
      "types": "./index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./tickets": {
      "types": "./tickets.d.ts",
      "import": "./dist/tickets/index.mjs",
      "require": "./dist/tickets/index.js"
    },
    "./leads": {
      "types": "./leads.d.ts",
      "import": "./dist/leads/index.mjs",
      "require": "./dist/leads/index.js"
    }
  },
  "scripts": {
    "typecheck": "tsc -p tsconfig.build.json --noEmit",
    "build": "tsup",
    "build:clean": "tsup --clean"
  },
  "dependencies": {
    "date-fns": "^3.0.6",
    "lodash": "^4.17.21",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.202",
    "tsup": "^8.5.0",
    "typescript": "^5.3.3"
  },
  "typesVersions": {
    "*": {
      "*": [
        "dist/*",
        "src/*"
      ]
    }
  },
  "files": [
    "dist",
    "index.d.ts",
    "tickets.d.ts",
    "leads.d.ts"
  ]
}
