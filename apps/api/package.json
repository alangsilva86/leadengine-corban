{
  "name": "@ticketz/api",
  "version": "1.0.0",
  "description": "API REST para o sistema Ticketz-LeadEngine",
  "main": "dist/server.mjs",
  "scripts": {
    "build": "tsup --clean && pnpm -w --filter @ticketz/storage run prisma:generate",
    "start": "node dist/server.mjs",
    "dev": "tsx watch src/server.ts"
  },
  "dependencies": {
    "@prisma/client": "5.22.0",
    "@ticketz/contracts": "workspace:*",
    "@ticketz/core": "workspace:*",
    "@ticketz/wa-contracts": "workspace:*",
    "@ticketz/shared": "workspace:*",
    "@aws-sdk/client-s3": "^3.712.0",
    "@aws-sdk/s3-request-presigner": "^3.712.0",
    "@ticketz/storage": "workspace:*",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "helmet": "^7.1.0",
    "multer": "^1.4.5-lts.1",
    "prom-client": "^15.1.1",
    "socket.io": "^4.7.4",
    "@whiskeysockets/baileys": "6.7.8",
    "undici": "^6.19.8",
    "winston": "^3.11.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/express-rate-limit": "5.1.3",
    "@types/express-validator": "3.0.2",
    "@types/multer": "^1.4.11",
    "tsup": "^8.5.0",
    "tsx": "^4.6.2",
    "supertest": "^7.0.0",
    "typescript": "^5.3.3",
    "vitest": "^1.6.0"
  }
}
